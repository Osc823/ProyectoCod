<div>
    <a href="/home">Inicio</a>
    <a href="/realTimeProducts">Crear Producto</a>
    <a href="/chat">Chat En vivo</a>
    
    
  <table class="table">
    <thead>
      <tr>
        <th scope="col"> Titulo </th>
        <th scope="col"> Descripcion </th>
        <th scope="col"> Precio </th>
        <th scope="col"> Imagen </th>
        <th scope="col"> Code </th>
        <th scope="col"> Stock </th>
      </tr>
    </thead>
    <tbody>
      {{#each products.docs}}
        <tr>
          <td> {{this.title}}</td>
          <td> {{this.description}}</td>
          <td> {{this.price}}</td>
          <td> {{this.thumbnail}} </td>
          <td> {{this.code}} </td>
          <td> {{this.stock}}</td>
        </tr>
      {{/each}}

    </tbody>

  </table>
  <div class="position-relative m-4">

    <nav aria-label="Page navigation example">
      <ul class="pagination">
        {{#if products.hasPrevPage}}
          <li class="page-item"><a
              class="page-link"
              href="/products?page={{products.prevPage}}&&limit=10"
            >Previous</a></li>
        {{/if}}

        {{!-- Links a siguientes p√°ginas --}}

        {{#if products.hasNextPage}}
          <li class="page-item"><a
              class="page-link"
              href="/products?page={{products.nextPage}}&&limit=10"
            >Next</a></li>
        {{/if}}

      </ul>
    </nav>
    <p class="btn btn-primary">
      Current Page:
      {{products.page}}
    </p>
  </div>
 
  </div>

 
</div>
<style>
a {
    color: #007bff;
    text-decoration: none;
    margin-right: 20px;
  }
</style>


