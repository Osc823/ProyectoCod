<div>
    <h1>Crear productos en tiempo real</h1>
    <a href="/">Inicio</a>
    <form>
        <input type="text" name="title" id="title" placeholder="Titulo">
        <input type="text" name="description" id="description" placeholder="Descripcion">
        <input type="number" name="price" id="price" placeholder="Precio">
        <input type="file" name="thumbnail" id="thumbnail" placeholder="Ingrese imagen">
        <input type="text" name="code" id="code" placeholder="AZS-152">
        <input type="number" name="stock" id="stock" placeholder="Stock "> 
        <input type="submit" value="Send">
    </form>
    <div id="productsRealTime"></div>

    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/main.js"></script>
</div>